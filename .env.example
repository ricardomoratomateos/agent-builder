# Node Environment
NODE_ENV=development

# Backend Configuration
BACKEND_PORT=3000

# Database Configuration
MONGODB_URI=mongodb://mongodb:27017/agent-builder

# Redis Configuration
REDIS_URL=redis://:your_secure_password_here@redis:6379
REDIS_PASSWORD=your_secure_password_here

# Qdrant Configuration
QDRANT_URL=http://qdrant:6333
QDRANT_API_KEY=

# Frontend Configuration
VITE_API_URL=http://localhost:3000
VITE_API_URL_PRODUCTION=https://api.yourdomain.com

# LLM API Keys (for testing and optimizer modules)
ANTHROPIC_API_KEY=sk-ant-your-key-here
OPENAI_API_KEY=sk-your-key-here

# JWT Configuration (for future auth implementation)
JWT_SECRET=your-secret-key-change-this-in-production
JWT_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGIN=http://localhost:3001
